#include <stdio.h>

int main (void) {
    int N; scanf("%d", &N);

    char SS[51*N];
    char *S[N];
    int len[N];

    int idx = 0;
    for (int i = 0; i < N; i++) {
        char tmp[51]; scanf("%s", tmp);
        int j = 0;
        for (; tmp[j] != '\0'; j++) {
            S[idx++] = tmp[j];
        }
        len[i] = j;
    }
}
